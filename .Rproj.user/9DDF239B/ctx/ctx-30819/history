# remove the year from talks, add months, order by months
library(knitr)
library(rmarkdown)
library(htmltools)
options(htmltools.dir.version = FALSE)
library(RefManageR)
articles <- ReadBib("papers/articles.bib", check = FALSE)
inproceedings <- ReadBib("papers/inproceedings.bib", check = FALSE)
presentations <- ReadBib("papers/others.bib", check = FALSE)
underreview <- ReadBib("papers/underreview.bib", check = FALSE)
books <- ReadBib("papers/books.bib", check = FALSE)
unpublished <- ReadBib("papers/unpublished.bib", check = FALSE)
endcit <- "</cite></p>"
this_year <- as.numeric(format(Sys.Date(), "%Y"))
first_year <-  2000
source("common.R")
BibOptions(check.entries = FALSE, style = "html", cite.style = "authortitle",
bib.style = "authoryear",sorting="ydnt",max.names =10,dashed=FALSE, no.print.fields=c("issn","eprint","url","customa","customb","customc"))
sort(articles,sorting="ydnt")
sort(inproceedings,sorting="ydnt")
sort(inproceedings,sorting="ydnt")
sort(presentations,sorting="ydnt")
sort(underreview,sorting="ydnt")
sort(books,sorting="ydnt")
sort(unpublished,sorting="ydnt")
endcit<- "</cite></p>"
this_year <- as.numeric(format(Sys.Date(), "%Y"))
first_year <-  2000
confs <- presentations[list(bibtype="inproceedings")]
confs <- presentations[list(bibtype="inproceedings")]
output(confs,withbrackets = FALSE)
